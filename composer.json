{
    "name": "Kajna/K",
    "description": "Simple and Lightweight but powerfull MVC framework",
    "authors": [
        {
        "name": "Milos Kajnaco",
        "email": "miloskajnaco@gmail.com"
        }
    ],
    "require":{
        "php": ">=5.4.0",
        "pimple/pimple": "3.0.*@dev",
        "phpunit/php-timer": "dev-master",
        "filp/whoops": "1.2.*@dev",
        "predis/predis": "1.1.*@dev"
    },
    "suggest": {
        "monolog/monolog": "Allows more advanced logging of the application flow",
        "swiftmailer/swiftmailer": "Great mailer library",
        "codeguy/upload": "Handle file uploads with extensible validation and storage strategies",
        "gregwar/image": "Image handling"
    },
    "autoload": {
        "psr-4": {
            "Core\\": "Core/",
            "Controllers\\": "App/Controllers/",
            "Models\\": "App/Models/"
        }
    }
}
